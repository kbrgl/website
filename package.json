{
  "name": "website",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "serve": "concurrently --kill-others --raw npm:tailwind npm:eleventy",
    "tailwind": "tailwindcss -w -i ./styles/app.css -o ./_site/app.css",
    "eleventy": "eleventy --serve --quiet --incremental",
    "build": "tailwindcss -m -i ./styles/app.css -o ./_site/app.css && eleventy"
  },
  "keywords": [],
  "author": "Kabir Goel",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-fetch": "^3.0.0",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@tailwindcss/typography": "^0.5.2",
    "airtable": "^0.11.4",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "dotenv": "^16.0.1",
    "lodash": "^4.17.21",
    "luxon": "^2.4.0",
    "markdown-it-footnote": "^3.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.1.2"
  }
}
